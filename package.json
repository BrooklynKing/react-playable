{
  "name": "react-playable",
  "version": "1.0.0",
  "description": "React wrapper for playable",
  "keywords": [
    "react",
    "adaptive",
    "dash",
    "hls",
    "html5",
    "media",
    "mpeg",
    "mse",
    "player",
    "video"
  ],
  "homepage": "https://BrooklynKing.github.io/react-playable/",
  "bugs": {
    "url": "https://github.com/wix/react-playable/issues",
    "email": "oleksiim@wix.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:wix/playable.git"
  },
  "license": "MIT",
  "author": {
    "name": "Oleksii Makodzeba",
    "email": "oleksiim@wix.com"
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/statics/react-playable.bundle.js",
  "scripts": {
    "start": "haste-playable start",
    "lint": "npm run tslint:all",
    "bundle:react-playable": "haste-playable bundle react-playable=./index.ts",
    "build": "haste-playable clean && haste-playable build && npm run bundle:react-playable",
    "test": "",
    "tslint": "ts-node node_modules/.bin/tslint -p . -c tslint.json",
    "tslint:all": "npm run tslint -- '{src,scripts}/**/*.{ts,tsx}'",
    "prettier:all": "prettier --write '{src,scripts,tslint-rules,dev-env}/**/*.{ts,tsx}'",
    "csscomb:all": "csscomb src/.",
    "precommit": "lint-staged && npm run test"
  },
  "peerDependencies": {
    "react": ">=16.1.0",
    "react-dom": ">=16.1.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "playable": "^2.1.1",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@babel/types": "7.0.0-beta.46",
    "@types/node": "^10.9.3",
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "chance": "^1.0.16",
    "conventional-changelog-cli": "^2.0.5",
    "csscomb": "^4.2.0",
    "gh-pages": "^1.2.0",
    "haste-preset-playable": "github:bodia-uz/haste-preset-playable#v2.2.1",
    "husky": "^0.14.3",
    "jsdom": "^11.12.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "prop-types": "^15.6.2",
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.0.1"
  },
  "publishConfig": {
    "logLevel": "verbose",
    "registry": "https://registry.npmjs.org/"
  },
  "haste": {
    "preset": "playable",
    "presetConfig": {
      "exports": "ReactPlayable",
      "devServer": {
        "entry": "./develop.tsx",
        "port": 5000
      }
    }
  },
  "lint-staged": {
    "*.tsx": [
      "prettier --write",
      "npm run tslint --",
      "git add"
    ],
    "*.ts": [
      "prettier --write",
      "npm run tslint --",
      "git add"
    ],
    "*.scss": [
      "csscomb -tty-mode --",
      "git add"
    ]
  }
}
